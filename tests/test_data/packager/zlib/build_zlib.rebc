!rebuild.recipe!

package zlib 1.2.8-1
  properties

  steps  
    step_autoconf
      tarball
        all: ${REBUILD_PACKAGE_NAME}-${REBUILD_PACKAGE_UPSTREAM_VERSION}.tar.gz dest=${REBUILD_BUILD_DIR}
      configure_env
        all: CFLAGS=${REBUILD_COMPILE_CFLAGS}
      configure_flags
        all: --static
